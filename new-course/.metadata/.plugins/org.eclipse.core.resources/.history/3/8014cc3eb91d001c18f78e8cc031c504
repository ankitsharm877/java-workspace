package demo.exceptions.handling;

public class TestMultipleCatchBlock {

	public static void main(String[] args) {
		try {
			int A[] = {1,0,3,4,5};
			int r;
			r = A[0] / A[1];
			System.out.println(r);
		} catch (ArithmeticException ae) {
			ae.printStackTrace();
		} catch (IndexOutOfBoundsException ioe) {
			ioe.printStackTrace();
		}

	}

}
